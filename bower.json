{
  "name": "jquery-ajax-alerts",
  "version": "1.1.0",
  "homepage": "https://github.com/geocrest/jquery-ajax-alerts",
  "authors": [
    "Geocrest Mapping, LLC"
  ],
  "description": "Provides some functionality for displaying Bootstrap alert messages when working with jQuery Ajax methods and, in particular, ASP.NET MVC Ajax forms.",
  "main": [
    "dist/js/jquery-ajax-alerts.min.js",
    "dist/css/jquery-ajax-alerts.min.css"
  ],
  "moduleType": [
    "amd",
    "globals"
  ],
  "keywords": [
    "asp.net mvc",
    "unobtrusive",
    "jquery",
    "ajax",
    "bootstrap",
    "alert"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "dist/js/build.txt",
    "jsdoc.conf.json",
    "Geocrest.Web.AjaxAlerts.nuspec"
  ],
  "dependencies": {
    "jquery": ">=1.9.1",
    "bootstrap": ">=2.3.2 <4.0.0-alpha.1"
  }
}
